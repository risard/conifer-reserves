<script language='javascript'>

	function doLogout() {

		var ses = cookieManager.read(COOKIE_SES);

		if(ses) {
      	var req = new RemoteRequest(
				"open-ils.auth",
				"open-ils.auth.session.remove", ses );
      	req.send(true);
		}

   cookieManager.remove(COOKIE_SES);
	location.href = location.href.replace( /(\?.*$)/, "" );

}

</script>


