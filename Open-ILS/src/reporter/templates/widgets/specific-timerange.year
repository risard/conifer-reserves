[%

USE date;
USE start = DateTime(now = 1);

PROCESS inputs;

WRAPPER html/table ;

	WRAPPER html/row;
		INCLUDE html/cell content='Year';
	END;

	WRAPPER html/row;
		WRAPPER html/cell;
			INCLUDE text name=input_prefix size=4 value=(current_value ? current_value : start.year);
		END;
	END;

END;

%]
