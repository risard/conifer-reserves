[%

PROCESS inputs;

BLOCK editor;
WRAPPER select name=input_prefix;
	count = 1;
        IF current_value;
                def = current_value;
        END;
	WHILE count < 53;
                INCLUDE option value=count selected=(count == def ? 1 : 0);
		count = count + 1;
	END;
END;
' weeks before runtime';
END;

BLOCK display;
current_value _ ' weeks before runtime';
END;
%]
